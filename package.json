{
  "name": "dokazovi-fe",
  "version": "0.5.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@reduxjs/toolkit": "^1.4.0",
    "@testing-library/jest-dom": "^5.11.4",
    "axios": "^0.21.0",
    "dompurify": "^2.2.6",
    "lodash": "^4.17.20",
    "qs": "^6.9.4",
    "quill": "^1.3.7",
    "quill-image-resize-module-react": "^3.0.0",
    "quill-image-uploader": "^1.2.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.12.2",
    "react-masonry-css": "^1.0.14",
    "react-quill": "2.0.0-beta.2",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-slick": "^0.27.13",
    "react-toastify": "^7.0.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "slick-carousel": "^1.8.1",
    "typescript": "~4.0.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .tsx,.ts src/ --quiet",
    "lint:fix": "eslint --ext .tsx,.ts src/ --quiet --fix",
    "prettier-format": "prettier --write",
    "prettier-format:all": "prettier --write 'src/**/*.{js,jsx,json,ts,tsx,css,md}'",
    "assemble": "yarn install --immutable && npm run lint && npm run build --if-present && npm test",
    "coverage": "npm test -- --coverage --watchAll=false"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/dompurify": "^2.1.0",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/node": "^12.0.0",
    "@types/qs": "^6.9.5",
    "@types/quill": "^1.3.10",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-slick": "^0.23.4",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.0.0",
    "husky": "^4.3.0",
    "jest": "26.6.0",
    "lint-staged": "^10.5.1",
    "prettier": "2.1.2",
    "ts-jest": "^26.4.4"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint",
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,ts,tsx,css,md}": "prettier --write"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  }
}
